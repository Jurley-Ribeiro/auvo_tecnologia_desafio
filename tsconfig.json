{
    "compilerOptions": {
      "target": "ESNext",
      "module": "CommonJS",
      "lib": ["ESNext"],
      "types": ["node", "playwright"],
      "moduleResolution": "node",
      "strict": true,
      "esModuleInterop": true,
      "resolveJsonModule": true
    },
    "include": ["**/*.ts"],
    "exclude": ["node_modules", "playwright-report", "test-results"]
  }
  